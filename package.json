{
  "name": "masoneffect",
  "version": "1.0.30",
  "description": "A library that provides particle morphing effects. It can be used with React, Vue, Svelte, and vanilla JavaScript.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.mjs",
      "require": "./dist/vue/index.cjs",
      "types": "./dist/vue/index.d.ts"
    },
    "./svelte": {
      "import": "./dist/svelte/index.mjs",
      "require": "./dist/svelte/index.cjs",
      "types": "./dist/svelte/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs && npm run build:vue && npm run build:svelte && node scripts/generate-react-types.js",
    "build:vue": "vite build --config vite.config.vue.mjs",
    "build:svelte": "vite build --config vite.config.svelte.mjs",
    "dev": "rollup -c rollup.config.mjs -w",
    "serve": "npx http-server . -p 8080 -o --cors",
    "dev:example": "npm run serve",
    "precheck": "node scripts/check-before-publish.js",
    "prepublishOnly": "npm run build && npm run precheck"
  },
  "keywords": [
    "particles",
    "morphing",
    "canvas",
    "animation",
    "react",
    "vue",
    "vanilla-js"
  ],
  "author": "mason.dev fe.hyunsu@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fe-hyunsu/masoneffect.git"
  },
  "homepage": "https://masoneffect.com",
  "bugs": {
    "url": "https://github.com/fe-hyunsu/masoneffect/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.9.0",
    "svelte": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || >=19.0.0-0",
    "svelte": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  }
}
