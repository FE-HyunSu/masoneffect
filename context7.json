{
  "name": "masoneffect",
  "version": "2.0.5",
  "description": "A collection of animation effects library. Supports React, Vue, Svelte, and vanilla JavaScript with Tree-shaking support.",
  "type": "npm-package",
  "packageManager": "npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/fe-hyunsu/masoneffect.git"
  },
  "homepage": "https://masoneffect.com",
  "license": "MIT",
  "author": "mason.dev fe.hyunsu@gmail.com",
  "keywords": [
    "animation",
    "effects",
    "particles",
    "morphing",
    "count",
    "canvas",
    "react",
    "vue",
    "svelte",
    "vanilla-js",
    "tree-shaking",
    "typescript"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./textToParticle": {
      "import": "./dist/textToParticle/index.mjs",
      "require": "./dist/textToParticle/index.cjs",
      "types": "./dist/textToParticle/index.d.ts"
    },
    "./count": {
      "import": "./dist/count/index.mjs",
      "require": "./dist/count/index.cjs",
      "types": "./dist/count/index.d.ts"
    },
    "./typing": {
      "import": "./dist/typing/index.mjs",
      "require": "./dist/typing/index.cjs",
      "types": "./dist/typing/index.d.ts"
    },
    "./slide": {
      "import": "./dist/slide/index.mjs",
      "require": "./dist/slide/index.cjs",
      "types": "./dist/slide/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    },
    "./react/textToParticle": {
      "import": "./dist/react/textToParticle/index.mjs",
      "require": "./dist/react/textToParticle/index.cjs",
      "types": "./dist/react/textToParticle/index.d.ts"
    },
    "./react/count": {
      "import": "./dist/react/count/index.mjs",
      "require": "./dist/react/count/index.cjs",
      "types": "./dist/react/count/index.d.ts"
    },
    "./react/typing": {
      "import": "./dist/react/typing/index.mjs",
      "require": "./dist/react/typing/index.cjs",
      "types": "./dist/react/typing/index.d.ts"
    },
    "./react/slide": {
      "import": "./dist/react/slide/index.mjs",
      "require": "./dist/react/slide/index.cjs",
      "types": "./dist/react/slide/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.mjs",
      "require": "./dist/vue/index.cjs",
      "types": "./dist/vue/index.d.ts"
    },
    "./vue/textToParticle": {
      "import": "./dist/vue/textToParticle/index.mjs",
      "require": "./dist/vue/textToParticle/index.cjs",
      "types": "./dist/vue/textToParticle/index.d.ts"
    },
    "./vue/count": {
      "import": "./dist/vue/count/index.mjs",
      "require": "./dist/vue/count/index.cjs",
      "types": "./dist/vue/count/index.d.ts"
    },
    "./vue/typing": {
      "import": "./dist/vue/typing/index.mjs",
      "require": "./dist/vue/typing/index.cjs",
      "types": "./dist/vue/typing/index.d.ts"
    },
    "./vue/slide": {
      "import": "./dist/vue/slide/index.mjs",
      "require": "./dist/vue/slide/index.cjs",
      "types": "./dist/vue/slide/index.d.ts"
    },
    "./svelte": {
      "import": "./dist/svelte/index.mjs",
      "require": "./dist/svelte/index.cjs",
      "types": "./dist/svelte/index.d.ts"
    },
    "./svelte/textToParticle": {
      "import": "./dist/svelte/textToParticle/index.mjs",
      "require": "./dist/svelte/textToParticle/index.cjs",
      "types": "./dist/svelte/textToParticle/index.d.ts"
    },
    "./svelte/count": {
      "import": "./dist/svelte/count/index.mjs",
      "require": "./dist/svelte/count/index.cjs",
      "types": "./dist/svelte/count/index.d.ts"
    },
    "./svelte/typing": {
      "import": "./dist/svelte/typing/index.mjs",
      "require": "./dist/svelte/typing/index.cjs",
      "types": "./dist/svelte/typing/index.d.ts"
    },
    "./svelte/slide": {
      "import": "./dist/svelte/slide/index.mjs",
      "require": "./dist/svelte/slide/index.cjs",
      "types": "./dist/svelte/slide/index.d.ts"
    }
  },
  "features": {
    "treeShaking": true,
    "typescript": true,
    "frameworks": ["react", "vue", "svelte", "vanilla"],
    "visibilityManagement": {
      "intersectionObserver": true,
      "pageVisibility": true,
      "gpuOptimization": true,
      "description": "All effects automatically pause when not visible or when tab is hidden to optimize GPU usage"
    },
    "effects": [
      {
        "name": "textToParticle",
        "description": "Converts text into animated particles with morphing effects",
        "keywords": ["text", "particle", "morph", "title", "banner", "hero"],
        "useCases": [
          "Hero section titles",
          "Loading screens",
          "Banner text",
          "Interactive text effects",
          "Text morphing animations"
        ]
      },
      {
        "name": "count",
        "description": "Animated number counting with intersection observer support",
        "keywords": ["number", "count", "counter", "stat", "statistic"],
        "useCases": [
          "Statistics display",
          "Number counters",
          "Animated numbers",
          "Scroll-triggered counters",
          "Performance metrics"
        ]
      },
      {
        "name": "typing",
        "description": "Typing animation effect with Korean character decomposition",
        "keywords": ["typing", "type", "typewriter", "terminal", "code", "korean", "한글"],
        "useCases": [
          "Code demonstrations",
          "Terminal simulations",
          "Typing effects",
          "Text reveal animations",
          "Korean text typing with consonant/vowel decomposition"
        ],
        "special": "Korean characters are decomposed into consonants and vowels for realistic typing effect"
      },
      {
        "name": "slide",
        "description": "Slide-in animation effect with direction control and flexible distance units",
        "keywords": ["slide", "slide-in", "reveal", "entrance", "animation", "scroll"],
        "useCases": [
          "Content reveal animations",
          "Scroll-triggered animations",
          "Entrance effects",
          "Directional slide animations",
          "Page section animations"
        ],
        "special": "Supports multiple directions (top, right, bottom, left) and flexible distance units (px, rem, em, %, vh, vw)"
      }
    ]
  },
  "structure": {
    "source": "src/",
    "core": "src/core/",
    "utils": "src/core/utils/",
    "react": "src/react/",
    "vue": "src/vue/",
    "svelte": "src/svelte/",
    "examples": "examples/",
    "scripts": "scripts/",
    "dist": "dist/"
  },
  "importantFiles": {
    "primaryAiGuide": "llms.txt",
    "readme": "README.md",
    "package": "package.json",
    "cursorRules": ".cursorrules",
    "copilotInstructions": ".github/copilot-instructions.md",
    "context7": "context7.json"
  },
  "importPaths": {
    "react": {
      "textToParticle": "masoneffect/react/textToParticle",
      "count": "masoneffect/react/count",
      "typing": "masoneffect/react/typing",
      "slide": "masoneffect/react/slide"
    },
    "vue": {
      "textToParticle": "masoneffect/vue/textToParticle",
      "count": "masoneffect/vue/count",
      "typing": "masoneffect/vue/typing",
      "slide": "masoneffect/vue/slide"
    },
    "svelte": {
      "textToParticle": "masoneffect/svelte/textToParticle",
      "count": "masoneffect/svelte/count",
      "typing": "masoneffect/svelte/typing",
      "slide": "masoneffect/svelte/slide"
    },
    "vanilla": {
      "textToParticle": "masoneffect/textToParticle",
      "count": "masoneffect/count",
      "typing": "masoneffect/typing",
      "slide": "masoneffect/slide"
    }
  },
  "build": {
    "core": "rollup",
    "config": "rollup.config.mjs",
    "vue": "vite",
    "svelte": "vite",
    "output": {
      "formats": ["esm", "cjs"],
      "extensions": {
        "esm": ".mjs",
        "cjs": ".cjs",
        "types": ".d.ts"
      }
    }
  },
  "peerDependencies": {
    "react": ">=16.8.0 || >=19.0.0-0",
    "vue": ">=3.0.0",
    "svelte": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "aiAgent": {
    "primaryGuide": "llms.txt",
    "detection": {
      "framework": "Check package.json dependencies for react/vue/svelte",
      "typescript": "Check for tsconfig.json or typescript in devDependencies",
      "buildTool": "Check for vite.config.js, webpack.config.js, or next in dependencies"
    },
    "recommendation": {
      "textToParticle": ["text", "particle", "morph", "title", "banner", "hero"],
      "count": ["number", "count", "counter", "stat", "statistic"],
      "typing": ["typing", "type", "typewriter", "terminal", "code", "korean", "한글"],
      "slide": ["slide", "slide-in", "reveal", "entrance", "animation", "scroll"]
    },
    "rules": [
      "Always use direct import paths for tree-shaking",
      "Use framework-specific import paths",
      "Import TypeScript types if TypeScript project",
      "Use correct prop naming (camelCase for React/Svelte, kebab-case for Vue)",
      "All effects automatically pause when not visible (IntersectionObserver) or when tab is hidden (Page Visibility API) for GPU optimization"
    ]
  },
  "installation": {
    "command": "npm install masoneffect",
    "alternatives": ["yarn add masoneffect", "pnpm add masoneffect"]
  }
}

